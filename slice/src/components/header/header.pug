include ../menu/menu
include ../b-social/b-social

//-
  options.menuItems - массив елементов меню

mixin header(options)
  if !options
    - options = {}

  header.header
    .header__social
      +b-social({items: options.socialItems})
    .container
      +menu({items: options.menuItems})
    .header__social.header__social-right
      +b-social({items: options.socialItems})